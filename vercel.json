{
  "version": 2,
  "builds": [
    {
      "src": "server/src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/index.tsx",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/src/index.ts" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
